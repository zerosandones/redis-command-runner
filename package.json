{
	"name": "redis-commander",
	"displayName": "redis-commander",
	"description": "redis client within vs code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.redis-commander.connect"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.redis-commander.connect",
				"title": "Connect",
				"category": "Redis Commander"
			},
			{
				"command": "extension.redis-commander.command",
				"title": "Command",
				"category": "Redis Commander"
			},
			{
				"command": "extension.redis-commander.disconnect",
				"title": "Disconnect",
				"category": "Redis Commander"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/redis": "^2.8.14",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"redis": "^2.8.0"
	}
}
